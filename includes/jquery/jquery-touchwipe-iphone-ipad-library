				function noSpam() {
				    var a = document.getElementsByTagName("a");
				    for (var i = a.length-1; i >= 0; i--) {
				      if (a[i].className.search(/\bemail\b/) != -1) {
				        var email = a[i].firstChild.data + "@" + a[i].lastChild.data;
				        a[i].innerHTML = email;
				        a[i].href = "mailto:" + email;
				      }
				    }
				}
				noSpam();
				</script>
